          width="120"
          height="120"
          viewBox="0 0 120 120"
          fill="none"
          xmlns="http://www.w3.org/2000/svg">
          <path d="M60 10L100 30V60C100 85 60 110 60 110S20 85 20 60V30L60 10Z" fill="#B88B34" />
          <path
            d="M45 60L52 67L75 44"
            stroke="white"
            stroke-width="4"
            stroke-linecap="round"
            stroke-linejoin="round" />
        </svg>
      </div>
      <h1 class="getsupp-hero-inline__title">{{ section.settings.hero_title | default: "Unlock Your Natural Potential" }}</h1>
      <p class="getsupp-hero-inline__subtitle">{{ section.settings.hero_subtitle | default: 'Shop 100% genuine health products from more than 300+ brands' }}</p>
      <!-- Sliding Product Banners -->
      <div class="getsupp-product-banner-slider">
        <div class="getsupp-product-banner-container">
          {%- liquid
            assign featured_products = collections[section.settings.featured_collection].products | limit: 6
            if featured_products.size == 0
              assign featured_products = collections.all.products | limit: 6
            endif
          -%}

          {% for product in featured_products %}
            <div class="getsupp-product-banner-item {% if forloop.first %}active{% endif %}" data-product-id="{{ product.id }}">
              <a href="{{ product.url }}" class="getsupp-product-banner-link">
                <div class="getsupp-product-banner-image">
                  {% if product.featured_image %}
                    <img
                      src="{{ product.featured_image | img_url: '300x300' }}"
                      alt="{{ product.title }}"
                      loading="lazy">
                  {% else %}
                    <div class="getsupp-product-banner-placeholder">
                      <svg
                        width="60"
                        height="60"
                        viewBox="0 0 24 24"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg">
                        <rect
                          x="3"
                          y="3"
                          width="18"
                          height="18"
                          rx="2"
                          stroke="currentColor"
                          stroke-width="2" />
                        <circle
                          cx="8.5"
                          cy="8.5"
                          r="1.5"
                          stroke="currentColor"
                          stroke-width="2" />
                        <path
                          d="M21 15L16 10L5 21"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round" />
                      </svg>
                    </div>
                  {% endif %}
                </div>
                <div class="getsupp-product-banner-content">
                  <h3 class="getsupp-product-banner-title">{{ product.title | truncate: 30 }}</h3>
                  <div class="getsupp-product-banner-price">
                    {% if product.compare_at_price > product.price %}
                      <span class="getsupp-product-banner-compare-price">{{ product.compare_at_price | money_without_currency }} INR</span>
                    {% endif %}
                    <span class="getsupp-product-banner-current-price">{{ product.price | money_without_currency }} INR</span>
                  </div>
                </div>
              </a>
            </div>
          {% endfor %}
        </div>

        <!-- Navigation Dots -->
        <div class="getsupp-product-banner-dots">
          {% for product in featured_products %}
            <button class="getsupp-product-banner-dot {% if forloop.first %}active{% endif %}" data-slide="{{ forloop.index0 }}"></button>
          {% endfor %}
        </div>
      </div>

    </div>
  </div>
{% endif %}


