  .getsupp-banner {
    background: linear-gradient(135deg, #24BAAD 0%, #1ea89a 50%, #0d9488 100%);
    padding: 12px 0;
    text-align: center;
    position: relative;
    overflow: hidden;
    animation: bannerPulse 6s ease-in-out infinite;
  }

  @keyframes bannerPulse {
    0,
    100% {
      background: linear-gradient(135deg, #24BAAD 0%, #1ea89a 50%, #0d9488 100%);
    }
    50% {
      background: linear-gradient(135deg, #1ea89a 0%, #24BAAD 50%, #1ea89a 100%);
