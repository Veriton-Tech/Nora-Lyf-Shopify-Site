  animation: float 25s ease-in-out infinite;
}

@keyframes float {
  0,
  100% {
    transform: translate(0, 0) rotate(0deg);
  }
  33% {
    transform: translate(40px, -40px) rotate(120deg);
  }
  66% {
    transform: translate(-30px, 30px) rotate(240deg);
  }
}

