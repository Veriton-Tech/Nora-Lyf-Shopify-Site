    color: #24BAAD;
    background: #eaf1f3;
    padding: 4px 8px;
    border-radius: 4px;
    margin: 0 0 8px;
    border-left: 3px solid #24BAAD;
  }

  .getsupp-mega-links {
    list-style: none;
    margin: 0;
    padding: 0;
  }

  .getsupp-mega-links li {
    margin-bottom: 4px;
  }

  .getsupp-mega-links a {
    display: block;
    padding: 4px 8px;
    font-size: 14px;
    color: #000;
    text-decoration: none;
    border-radius: 4px;
    transition: all 0.2s;
  }

  .getsupp-mega-links a:hover {
    color: #24BAAD;
    font-weight: bold;
    background: #f0fdfc;
  }

  /* Brands Menu Special Styling */
  .getsupp-brands-menu .getsupp-mega-content {
    grid-template-columns: 1fr;
    width: 320px;
    max-height: 70vh;
  }
  .getsupp-brands-menu {
    left: 50%;
    transform: translateX(-50%);
  }
  .getsupp-brands-search {
    position: sticky;
    top: 0;
    background: #ffffff;
    padding: 8px 0 4px;
  }
  .getsupp-brands-input {
    width: 100%;
    padding: 8px 10px;
    border: 1px solid #e5e5e5;
    border-radius: 8px;
    font-size: 14px;
    outline: none;
  }

  /* Enhanced Inline Hero below navigation */
  .getsupp-hero-inline {
    background: linear-gradient(135deg, #f8f9fa 0%, #e3f2fd 50%, #f0fdfc 100%);
    padding: 80px 0 60px;
    position: relative;
    overflow: hidden;
  }
  .getsupp-hero-inline::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: url('data:image/svg+xml, <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><defs><pattern id="grain" width="100" height="100" patternUnits="userSpaceOnUse"><circle cx="25" cy="25" r="1" fill="%2324BAAD" opacity="0.1"/><circle cx="75" cy="75" r="1" fill="%2324BAAD" opacity="0.1"/><circle cx="50" cy="10" r="0.5" fill="%2324BAAD" opacity="0.05"/><circle cx="10" cy="60" r="0.5" fill="%2324BAAD" opacity="0.05"/><circle cx="90" cy="40" r="0.5" fill="%2324BAAD" opacity="0.05"/></pattern></defs><rect width="100" height="100" fill="url(%23grain)"/></svg>');
    opacity: 0.3;
  }
  .getsupp-hero-inline__container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 24px;
    text-align: center;
    position: relative;
    z-index: 2;
  }
  .getsupp-hero-inline__icon {
    display: flex;
    justify-content: center;
    margin-bottom: 24px;
    animation: float 3s ease-in-out infinite;
  }
  @keyframes float {
    0,
    100% {
      transform: translateY(0px);
    }
    50% {
      transform: translateY(-10px);
    }
  }
  .getsupp-hero-inline__title {
    font-size: 48px;
    line-height: 1.2;
    font-weight: 800;
    margin: 0 0 16px;
    background: linear-gradient(135deg, #24BAAD 0%, #1ea89a 50%, #0d9488 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    animation: fadeInUp 0.8s ease-out;
  }
  @keyframes fadeInUp {
    from {
      opacity: 0;
      transform: translateY(30px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }
  .getsupp-hero-inline__links {
    display: flex;
    gap: 32px;
    justify-content: center;
    margin-bottom: 32px;
    flex-wrap: wrap;
  }
  .getsupp-hero-inline__link {
    color: #24BAAD;
    text-decoration: none;
    font-weight: 600;
    font-size: 16px;
    display: flex;
    align-items: center;
    gap: 8px;
    transition: all 0.3s ease;
    padding: 12px 20px;
    border-radius: 25px;
    background: rgba(36, 186, 173, 0.1);
    border: 2px solid transparent;
  }
  .getsupp-hero-inline__link:hover {
    color: white;
    background: #24BAAD;
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(36, 186, 173, 0.3);
  }
  .getsupp-hero-inline__subtitle {
    font-size: 18px;
    color: #666;
    margin-bottom: 40px;
    max-width: 700px;
    margin-left: auto;
    margin-right: auto;
    line-height: 1.6;
    animation: fadeInUp 0.8s ease-out 0.2s both;
  }
  .getsupp-hero-inline__search {
    max-width: 600px;
    margin: 0 auto;
    position: relative;
    animation: fadeInUp 0.8s ease-out 0.4s both;
  }
  .getsupp-hero-inline__search input {
    width: 100%;
    padding: 20px 24px;
    border: 2px solid #e5e5e5;
    border-radius: 50px;
    font-size: 16px;
    outline: none;
    transition: all 0.3s ease;
    background: white;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
  }
  .getsupp-hero-inline__search input:focus {
    border-color: #24BAAD;
    box-shadow: 0 0 0 4px rgba(36, 186, 173, 0.1)
    , 0 8px 30px rgba(0, 0, 0, 0.15);
    transform: translateY(-2px);
  }
  .getsupp-hero-inline__search input::placeholder {
    color: #999;
    font-weight: 400;
  }
  .getsupp-hero-inline__search button {
    position: absolute;
    right: 8px;
    top: 50%;
    transform: translateY(-50%);
    background: linear-gradient(135deg, #24BAAD 0%, #1ea89a 100%);
    color: white;
    border: none;
    padding: 12px 24px;
    border-radius: 25px;
    cursor: pointer;
    transition: all 0.3s ease;
    font-weight: 600;
    box-shadow: 0 4px 15px rgba(36, 186, 173, 0.3);
  }
  .getsupp-hero-inline__search button:hover {
    background: linear-gradient(135deg, #1ea89a 0%, #0d9488 100%);
    transform: translateY(-50%) scale(1.05);
    box-shadow: 0 6px 20px rgba(36, 186, 173, 0.4);
  }
  @media (max-width: 768px) {
    .getsupp-hero-inline {
      padding: 36px 0 20px;
    }
    .getsupp-hero-inline__title {
      font-size: 28px;
    }
    .getsupp-hero-inline__search {
      height: 56px;
    }
    .getsupp-hero-inline__search input {
      font-size: 16px;
    }
  }

  /* Mobile Responsive */
  @media (max-width: 1024px) {
    .getsupp-search {
      display: none;
    }

    .getsupp-mobile-search {
      display: block;
    }

    .getsupp-navigation {
      display: none;
    }
  }

  @media (min-width: 1025px) {
    .getsupp-mobile-menu {
      display: none;
    }

    .getsupp-mobile-search {
      display: none;
    }
  }
{% endstylesheet %}

