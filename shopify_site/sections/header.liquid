{% javascript %}
  (function() {
    try { // Set CSS var to keep dropdown just under header/navigation
      var nav = document.querySelector('.getsupp-navigation');
      function updateNavBottom() {
        var rect = nav
          ? nav.getBoundingClientRect()
          : null;
        var bottom = rect
          ? Math.max(0, Math.floor(rect.bottom))
          : 100;
        document.documentElement.style.setProperty('--getsupp-nav-bottom', bottom + 'px');
      }
      updateNavBottom();
      window.addEventListener('resize', updateNavBottom);
      window.addEventListener('scroll', updateNavBottom, {passive: true});

      var timerContainer = document.querySelector('.getsupp-banner-timer');
      if (! timerContainer) return;
      


      var endAttr = timerContainer.getAttribute('data-endtime');
      if (! endAttr || endAttr.trim() === '') return;
      


      var endTime = new Date(endAttr).getTime();
      if (isNaN(endTime)) return;
      


      var el = document.getElementById('getsupp-offer-timer');
      if (! el) return;
      


      function pad(n) {
        return n < 10
          ? '0' + n
          : '' + n;
      }
      function tick() {
        var now = Date.now();
        var diff = Math.max(0, endTime - now);
        var totalSeconds = Math.floor(diff / 1000);
        var hours = Math.floor(totalSeconds / 3600);
        var minutes = Math.floor((totalSeconds % 3600) / 60);
        var seconds = totalSeconds % 60;
        el.textContent = pad(hours) + 'h : ' + pad(minutes) + 'm : ' + pad(seconds) + 's';
        if (diff <= 0) 
          clearInterval(intervalId);
        


      }
      tick();
      var intervalId = setInterval(tick, 1000);
    } catch (e) { // no-op
    }
  })();
{% endjavascript %}

