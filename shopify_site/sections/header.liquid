.getsupp-new-arrivals::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: url('data:image/svg+xml, <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><defs><pattern id="grain" width="100" height="100" patternUnits="userSpaceOnUse"><circle cx="25" cy="25" r="1.5" fill="%2304683F" opacity="0.12"/><circle cx="75" cy="75" r="1.5" fill="%23B88B34" opacity="0.12"/><circle cx="50" cy="10" r="0.8" fill="%2304683F" opacity="0.08"/><circle cx="10" cy="60" r="0.8" fill="%23B88B34" opacity="0.08"/><circle cx="90" cy="40" r="0.8" fill="%2304683F" opacity="0.08"/><circle cx="30" cy="80" r="0.6" fill="%23B88B34" opacity="0.06"/><circle cx="70" cy="20" r="0.6" fill="%2304683F" opacity="0.06"/></pattern></defs><rect width="100" height="100" fill="url(%23grain)"/></svg>');
  pointer-events: none;
}

.getsupp-new-arrivals::after {
  content: '';
  position: absolute;
  top: -50%;
  left: -50%;
  width: 200%;
  height: 200%;
  background: radial-gradient(circle at 20% 30%, rgba(4, 104, 63, 0.03) 0%, transparent 50%), radial-gradient(circle at 80% 70%, rgba(184, 139, 52, 0.03) 0%, transparent 50%);
